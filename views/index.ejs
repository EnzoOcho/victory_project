                                                        <div class="<%=type%>">
                                                            <%= clase %>
                                                        </div>
                                                        <% }); %>
            </div>
            <div class="dia">
                <div class="day">Miercoles</div>
                <% clases.miercoles.forEach(clase=> { %>
                    <% let type="black" %>
                        <% if(clase==="Funcional y Crossfit" ){%>
                            <%type="func_cross" %>
                                <% } else if(clase==="Crossfit" ){ %>
                                    <% type="cross" %>
                                        <%} else if(clase==="Funcional" ){ %>
                                            <% type="func" %>
                                                <% } else if(clase==="Openbox" ){ %>
                                                    <% type="open" } %>
                                                        <div class="<%=type%>">
                                                            <%= clase %>
                                                        </div>
                                                        <% }); %>
            </div>
            <div class="dia">
                <div class="day">Jueves</div>
                <% clases.jueves.forEach(clase=> { %>
                    <% let type="black" %>
                        <% if(clase==="Funcional y Crossfit" ){%>
                            <%type="func_cross" %>
                                <% } else if(clase==="Crossfit" ){ %>
                                    <% type="cross" %>
                                        <%} else if(clase==="Funcional" ){ %>
                                            <% type="func" %>
                                                <% } else if(clase==="Openbox" ){ %>
                                                    <% type="open" } %>
                                                        <div class="<%=type%>">
                                                            <%= clase %>
                                                        </div>
                                                        <% }); %>
            </div>
            <div class="dia">
                <div class="day">Viernes</div>
                <% clases.viernes.forEach(clase=> { %>
                    <% let type="black" %>
                        <% if(clase==="Funcional y Crossfit" ){%>
                            <%type="func_cross" %>
                                <% } else if(clase==="Crossfit" ){ %>
                                    <% type="cross" %>
                                        <%} else if(clase==="Funcional" ){ %>
                                            <% type="func" %>
                                                <% } else if(clase==="Openbox" ){ %>
                                                    <% type="open" } %>
                                                        <div class="<%=type%>">
                                                            <%= clase %>
                                                        </div>
                                                        <% }); %>
            </div>
            <div class="dia">
                <div class="day">Sabado</div>
                <% clases.sabado.forEach(clase=> { %>
                    <% let type="black" %>
                        <% if(clase==="Funcional y Crossfit" ){%>
                            <%type="func_cross" %>
                                <% } else if(clase==="Crossfit" ){ %>
                                    <% type="cross" %>
                                        <%} else if(clase==="Funcional" ){ %>
                                            <% type="func" %>
                                                <% } else if(clase==="Openbox" ){ %>
                                                    <% type="open" } %>
                                                        <div class="<%=type%>">
                                                            <%= clase %>
                                                        </div>
                                                        <% }); %>
            </div>
        </div>
    </section>
    <section id="Ubicación" class="ubication-page">
